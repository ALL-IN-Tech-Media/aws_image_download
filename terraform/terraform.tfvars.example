# Example Terraform variables file
# Copy this to terraform.tfvars and customize for your environment

# Basic Configuration
aws_region              = "us-east-1"
project_name           = "tiktok"
environment            = "prod"
lambda_function_name   = "image-collage-processor"

# Public Access Configuration
# Set to true if you want generated collages to be publicly accessible
enable_public_collages = false

# Log Retention
# How long to keep CloudWatch logs (in days)
log_retention_days = 30
